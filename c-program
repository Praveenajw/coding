CHAR PROGRAM DISPALY IN STRING
..............................................

#include<stdio.h>

int main()
{

	char a[50];
 	char b[50];
 	char c[50];
    printf("enter a word:\n");
    scanf("%s\n",a);
	printf("enter a word:\n");
    scanf("%s\n",b);
	printf("enter a word:\n");
	scanf("%s\n",c);
	return 0;
}
.............................................



COPY STRING
.............................................


#include <stdio.h>

int main()
{
    char str[100], copystr[100];
    int i=0;
    printf("enter");
    scanf("%s",str);
    while(str[i]!='\0')
    {
        copystr[i]=str[i];
        i++;
    }
   copystr[i]='\0';
   printf("copy word%s",copystr);
    return 0;
}
